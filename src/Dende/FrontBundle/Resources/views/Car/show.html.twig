{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Car</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ entity.year }}</td>
            </tr>
            <tr>
                <th>Distance</th>
                <td>{{ entity.distance }}</td>
            </tr>
            <tr>
                <th>Fuel</th>
                <td>{{ entity.fuel }}</td>
            </tr>
            <tr>
                <th>Engine</th>
                <td>{{ entity.engine }}</td>
            </tr>
            <tr>
                <th>Gearbox</th>
                <td>{{ entity.gearbox }}</td>
            </tr>
            <tr>
                <th>Registrationcountry</th>
                <td>{{ entity.registrationCountry }}</td>
            </tr>
            <tr>
                <th>Promotecarousel</th>
                <td>{{ entity.promoteCarousel }}</td>
            </tr>
            <tr>
                <th>Promotefrontpage</th>
                <td>{{ entity.promoteFrontpage }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Adminnotes</th>
                <td>{{ entity.adminNotes }}</td>
            </tr>
            <tr>
                <th>Hidden</th>
                <td>{{ entity.hidden }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Modified</th>
                <td>{{ entity.modified|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Deleted</th>
                <td>{{ entity.deleted|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('car') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
