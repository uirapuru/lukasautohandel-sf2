<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="{{ app.request.getLocale() }}"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{% block title %}{% endblock %}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {% block stylesheets %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('css/vendors.css') }}"/>
            <link rel="stylesheet" type="text/css" href="{{ asset('css/project.css') }}"/>
        {% endblock %}
    </head>
    <body>
        <div class="container">
            <div class="row">
                {% block body %}{% endblock %}
            </div>
        </div>

    {% block javascripts %}
        <script src="{{ asset('js/vendors.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/project.js') }}" type="text/javascript"></script>
        {% include '::analytics.html.twig' %}
    {% endblock javascripts %}
</body>
</html>
